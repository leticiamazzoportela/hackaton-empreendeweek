          <div class="container">
            
            <div class="row justify-content-md-center">
              <div class="col-sm-10 col-md-10 col-lg-10">
                <h1> Processos </h1>
                <button type = "button" class="btn btn-outline-info" onclick="showButton()">Filtros</button>
                <div class="search-container" id='searchBar'>
                    <input type="text" placeholder="Digite o texto do filtro" name="search">
                    <button type="submit" style="margin-left:-6px;"><i class="fa fa-search"></i></button>
                    <select class="form-control col-sm-2">
                      <option value="ID">#</option>
                      <option value="Reclamante">Reclamante</option>
                      <option value="Reclamado">Reclamado</option>
                      <option value="Data">Data</option>
                      <option value="Status">Status</option>
                      <option value="enquad1">Area</option>
                      <option value="enquad2">Assunto</option>
                      <option value="enquad3">Problema</option>
                      <option value="atend_forma">Forma de atendimento</option>
                      <option value="atend_tipo">Tipo atendimento</option>

                    </select>
                </div>

                    <table class="table table-hover">
                        <thead>
                          <tr>
                            <th scope="col">#</th>
                            <th scope="col">Reclamante</th>
                            <th scope="col">Reclamado</th>
                            <th scope="col">Data</th>
                            <th scope="col">Status</th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr>
                            <th scope="row">1</th>
                            <td>Maria Costa</td>
